		"ios": "expo start --ios",
		"test": "jest --watchAll",
		"ci": "jest",
		"app": "cp apps/app.ais.json app.json && expo r -c",
		"g": "expo start --config apps/app.generic.json --ios",
		"offline": "expo start --config apps/app.generic.json --ios --offline",
